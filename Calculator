    def equal(self):
        mytext=self.calculate.text()
        mytext = mytext.replace('X','*')
        mytext = mytext.replace('รท','/')
        self.answer.setText("Answer="+str(eval(mytext)))
    def delete_all(self):
        self.calculate.setText("")
    def back(self):
        mytext=self.calculate.text()
        newtext=mytext[0:len(mytext)-1]
        self.calculate.setText(newtext)

