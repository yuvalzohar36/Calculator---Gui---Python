    def multi(self):
        mytext=self.calculate.text()
        self.calculate.setText(mytext+"X")
    def div(self):
        mytext=self.calculate.text()
        self.calculate.setText(mytext+"รท")
    def plus(self):
        mytext=self.calculate.text()
